<h1>Create new File</h1>

<%= form_with scope: :article, url: articles_path , local: true do |f| %>
  <p>
    <%= f.label :title, "Title" %><br/>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description, "Description" %><br/>
    <%= f.text_area :description %>
  </p>
  <p><%= f.submit %> </p>
<% end %>
