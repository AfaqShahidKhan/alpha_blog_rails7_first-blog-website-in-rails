<h1>Article listing page</h1>
<table>

<thead>

<tr>
<td>Title</td>
<td>Description</td>
<td>Actoin</td>
</tr>
</thead>
<tbody>
<% @articles.each do |article|  %>
<tr>
<td><%= article.title %></td>
<td> <%= article.description %></td>
<td><%= link_to "Show", article_path(article) %></td>
<td><%= link_to "Delete", article_path(article), method: :delete%></td>

<td>placeholder</td>
</tr>
<% end %>
</tbody>
</table>
